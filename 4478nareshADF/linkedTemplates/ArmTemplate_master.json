{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "4478nareshADF"
		},
		"LS_blbsrc_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blbsrc'"
		},
		"LS_gen2dest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_gen2dest'"
		},
		"LS_gen2dest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://4478destsa.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "4478nareshADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_blbsrc_connectionString": {
						"value": "[parameters('LS_blbsrc_connectionString')]"
					},
					"LS_gen2dest_accountKey": {
						"value": "[parameters('LS_gen2dest_accountKey')]"
					},
					"LS_gen2dest_properties_typeProperties_url": {
						"value": "[parameters('LS_gen2dest_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}